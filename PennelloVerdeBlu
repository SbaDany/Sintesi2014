/* prova 28-10-14 */
/* spruzzi verdi e blu */

void setup(){
  size(400,400);
  background(0);
}

void draw(){
  //background(0);
  for(int i=0; i<01; i++){
    float f=random(10); 
    float g=random(10);
    noStroke();
    fill(random(30,90),100+g,random(50,200));
    ellipse(mouseX+f,mouseY+g,10+f,10+f);
  }
}
